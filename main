local function GetRowOutOf(Frame:Frame)
	local Row:Frame = Frame:FindFirstChild("Row")
	return Row
end

local function SetupInterface()
	if not RunService:IsStudio() then
		CoreGui = game:GetService("CoreGui")
	else
		warn("You're running SimpleLibrary in studio.")
	end
	
	GetRowOutOf(SerializedTable.ContainerHolder.holder).textbox.Visible = false
	SerializedTable.ContainerHolder.Visible = false
	SerializedTable.CategoryList.Row.Visible = false
	SerializedTable.ContainerHolder.holder.Row.Visible = false
	SerializedTable.MainUI_Dashboard.Parent = CoreGui
	
	SerializedTable.g_Close.InputBegan:Connect(function(k)
		if k.UserInputType==Enum.UserInputType.MouseButton1 then
			SerializedTable.MainUI_Dashboard:Destroy()
		end
	end)
end


--// Library Functions

function module.Title(Name:string)
	SerializedTable.MainFrame.TextLabel.Text = Name
end

function module.CreateCategory(Name:string)
	local ListButton:TextButton = GetRowOutOf(SerializedTable.CategoryList):Clone()
	local Container:Frame = SerializedTable.ContainerHolder:Clone()
	
	-- code
	
	
	ListButton.Visible = true
	ListButton.Name = Name..".Category"
	ListButton.Text = Name
	ListButton.Parent = SerializedTable.CategoryList
	
	Container.Name = Name..".Container"
	if not IsFirstContainerInitialized then
		IsFirstContainerInitialized = true
		Container.Visible = true
	end
	Container.Parent = SerializedTable.MainFrame

	table.insert(tableContainers, Container)
	
	ListButton.MouseButton1Down:Connect(function()
		for _,v in tableContainers do
			if v.Name~=Name..".Container" then
				v.Visible = false
			else
				v.Visible = true
			end
		end
	end)

	return Container
end

function module.CreateSubCategory(Container:Frame, Name:string)
	local SubContainer:Frame = SerializedTable.SubContainerHolder:Clone()
	local exp:TextButton = SubContainer.cname.exp
	local holder:Frame = SubContainer.holder

	-- code

	SubContainer.cname.Text = Name
	SubContainer.Name = Name..".SubContainer"
	SubContainer.Visible = true
	SubContainer.Parent = Container.holder
	
	local isexpanded = false
	exp.MouseButton1Down:Connect(function()
		if isexpanded then
			exp.Rotation = 0
			holder.Visible = false
			SubContainer:FindFirstChildWhichIsA("UIPadding").PaddingBottom = UDim.new(0, 5)
		else
			exp.Rotation = -90
			holder.Visible = true
			SubContainer:FindFirstChildWhichIsA("UIPadding").PaddingBottom = UDim.new(0, 0)
		end
		
		isexpanded = not isexpanded
	end)
	

	return SubContainer
end

function module.CreateRow(Container:Frame, Name:string, Style:string, Value:Instance)
	local ListRow:Frame = GetRowOutOf(SerializedTable.ContainerHolder.holder):Clone()
	local Label:TextButton = ListRow.gap
	
	Style = Style:lower()
	
	Label.Text = Name
	
table.insert(tableRows, ListRow)
	ListRow.Visible = true
	ListRow.Name = Name..".Row"
	ListRow.Parent = Container.holder
	
	if Style=="bool" or Style=="boolean" then
		local def = Value.Value
		local Value:BoolValue = Value
		local Sheet:Frame = ListRow.boolean
		Sheet.Visible = true
		
		local function upd()
			pcall(function()
				local i = Sheet:FindFirstChildWhichIsA("ImageLabel")
				if Value.Value then
					i.ImageTransparency = 0
					i.BackgroundColor3 = Color3.new(1, 1, 1)
				else
					i.ImageTransparency = 1
					i.BackgroundColor3 = Color3.fromRGB(21, 21, 21)
				end
			end)
		end
		upd()
		
		Label.MouseButton1Down:Connect(function()
			Value.Value = not Value.Value
		end)
		Value:GetPropertyChangedSignal("Value"):Connect(function()
			upd()
		end)
		SerializedTable.MainUI_Dashboard.Destroying:Connect(function()
			Value.Value = def
		end)
	end
	if Style=="string" or Style=="number" then
		local Value:BoolValue = Value
		local Sheet:Frame = ListRow.textbox
		Sheet.Visible = true
		local i = Sheet:FindFirstChildWhichIsA("TextBox")
		
		local previoustext = ""
		i:GetPropertyChangedSignal("Text"):Connect(function()
			previoustext = i.Text
		end)
		i.FocusLost:Connect(function()
			if Style=="string" then
				Value.Value = i.Text
			else
				local t = tonumber(i.Text)

				if t then
					Value.Value = t
				else
					Value.Value = 0
					i.Text = "0"
				end
			end
			
			i.Text = Value.Value
		end)
		Value:GetPropertyChangedSignal("Value"):Connect(function()
			if not i:IsFocused() then
				i.Text = Value.Value
			end
		end)
		
	end
	
	
	return Label
end



--// Code

SetupInterface()


return module
