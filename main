InsertSerial()

local function GetRowOutOf(Frame:Frame)
	local Row:Frame = Frame:FindFirstChild("Row")
	return Row
end

local function SetupInterface()
	if not RunService:IsStudio() then
		CoreGui = game:GetService("CoreGui")
	else
		warn("You're running SimpleLibrary in studio.")
	end
	
	GetRowOutOf(SerializedTable.ContainerHolder.holder).textbox.Visible = false
	SerializedTable.ContainerHolder.Visible = false
	SerializedTable.CategoryList.Row.Visible = false
	SerializedTable.MainFrame.Size = UDim2.fromScale(.5, .5)
	SerializedTable.ContainerHolder.holder.Row.Visible = false
	SerializedTable.MainUI_Dashboard.Parent = CoreGui
	SerializedTable.MainUI_Dashboard.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	
	SerializedTable.MainUI_Dashboard.DescendantAdded:Connect(function(c)
		local ap:ObjectValue = c:FindFirstChild("ActivationPoint")
		if ap then
			local c:TextButton = c
			local nh = c.BackgroundColor3
			
			local function subC3(c3:Color3, amn)
				return Color3.new(c3.R-amn, c3.G-amn, c3.B-amn)
			end
			
			c.MouseEnter:Connect(function()
				c.BackgroundColor3 = subC3(nh, 0.05)
			end)
			c.MouseButton1Down:Connect(function()
				c.BackgroundColor3 = subC3(nh, 0.1)
			end)
			c.MouseButton1Up:Connect(function()
				c.BackgroundColor3 = nh
			end)
			c.MouseLeave:Connect(function()
				c.BackgroundColor3 = nh
			end)
			
		end
	end)
	
	SerializedTable.g_Close.InputEnded:Connect(function(k)
		if k.UserInputType==Enum.UserInputType.MouseButton1 then
			SerializedTable.MainUI_Dashboard:Destroy()
		end
	end)
end


--// Library Functions

function module.GetDirection(UseProcssing:boolean)
	
end

function module.Title(Name:string)
	SerializedTable.MainFrame.bounder.Text = Name
end

function module.Notify(Title:string, TextContent:string, duration:number, Options)
	--pcall(function()
		if not duration then
			duration = 3
		end
		local optosel = nil
		
		local c:Frame = SerializedTable.gNotificationFrame.Row:Clone()
		c.Frame.titletxt.Text = TextContent
		c.titletxt.Text = Title
		c.Visible = true
		c.Parent = SerializedTable.gNotificationFrame
		
		if Options then
			c.ops.Visible = true
			for _,v in Options do
				local oprow:TextButton = c.ops.row:Clone()
				oprow.Text = v
				oprow.Visible = true
				oprow.Parent = c.ops
				
				oprow.MouseButton1Down:Connect(function()
					optosel = v
					c:Destroy()
				end)
			end
		end
		
		spawn(function()
			task.wait(duration)
			if c and c.Parent then
				c:Destroy()
			end
		end)

		c.titletxt.ext.MouseButton1Down:Connect(function()
			c:Destroy()
		end)
		
		if Options then
			repeat
				task.wait()
			until optosel or c.Parent==nil
			return optosel
		end
--	end)
end

function module.CreateCategory(Name:string)
	local ListButton:TextButton = GetRowOutOf(SerializedTable.CategoryList):Clone()
	local Container:Frame = SerializedTable.ContainerHolder:Clone()
	
	-- code
	
	
	ListButton.Visible = true
	ListButton.Name = Name..".Category"
	ListButton.Text = Name
	ListButton.Parent = SerializedTable.CategoryList
	
	Container.Name = Name..".Container"
	if not IsFirstContainerInitialized then
		IsFirstContainerInitialized = true
		Container.Visible = true
	end
	Container.Parent = SerializedTable.MainFrame

	table.insert(tableContainers, Container)
	
	ListButton.MouseButton1Down:Connect(function()
		for _,v in tableContainers do
			if v.Name~=Name..".Container" then
				v.Visible = false
			else
				v.Visible = true
			end
		end
	end)

	return Container
end

function module.CreateSubCategory(Container:Frame, Name:string)
	local SubContainer:Frame = SerializedTable.SubContainerHolder:Clone()
	local exp:TextButton = SubContainer.cname.exp
	local holder:Frame = SubContainer.holder

	-- code

	SubContainer.cname.Text = Name
	SubContainer.Name = Name..".SubContainer"
	SubContainer.Visible = true
	SubContainer.Parent = Container.holder
	
	local isexpanded = false
	exp.MouseButton1Down:Connect(function()
		if isexpanded then
			exp.Rotation = 0
			holder.Visible = false
			SubContainer:FindFirstChildWhichIsA("UIPadding").PaddingBottom = UDim.new(0, 5)
		else
			exp.Rotation = -90
			holder.Visible = true
			SubContainer:FindFirstChildWhichIsA("UIPadding").PaddingBottom = UDim.new(0, 0)
		end
		
		isexpanded = not isexpanded
	end)
	

	return SubContainer
end

function module.CreateRow(Container:Frame, Name:string, Style:string, Value:Instance)
	local ListRow:Frame = GetRowOutOf(SerializedTable.ContainerHolder.holder):Clone()
	local Label:TextButton = ListRow.gap
	
	Style = Style:lower()
	
	Label.Text = Name
	
table.insert(tableRows, ListRow)
	ListRow.Visible = true
	ListRow.Name = Name..".Row"
	ListRow.Parent = Container.holder
	
	if Style=="bool" or Style=="boolean" then
		local def = Value.Value
		local Value:BoolValue = Value
		local Sheet:Frame = ListRow.boolean
		Sheet.Visible = true
		
		local function upd()
			pcall(function()
				local i = Sheet:FindFirstChildWhichIsA("ImageLabel")
				if Value.Value then
					i.ImageTransparency = 0
					i.BackgroundColor3 = Color3.new(1, 1, 1)
				else
					i.ImageTransparency = 1
					i.BackgroundColor3 = Color3.fromRGB(21, 21, 21)
				end
			end)
		end
		upd()
		
		Label.MouseButton1Down:Connect(function()
			Value.Value = not Value.Value
		end)
		Value:GetPropertyChangedSignal("Value"):Connect(function()
			upd()
		end)
		SerializedTable.MainUI_Dashboard.Destroying:Connect(function()
			Value.Value = def
		end)
	end
	if Style=="dropdown" or Style=="drop" or Style=="select" then
		local sheet:Frame = ListRow.dropdown
		sheet.Visible = true
		local tb:TextBox = sheet.ext
		local val = nil
		tb.BorderSizePixel = 1
		tb:FindFirstChildWhichIsA("UICorner"):Destroy()
	--	tb.BorderColor3 = Color3.fromRGB(34, 34, 34)
		local valname = nil
		tb.Interactable = false

		local function upd()
			tb.Text = tostring(valname)
		end
		
		upd()
		
		
		Label.InputBegan:Connect(function(k)
			if k.UserInputType==Enum.UserInputType.MouseButton1 then
				local ui = SerializedTable.gdropdownexamplerowford:Clone()
				ui.Position = UDim2.fromScale(0, 0)
				ui.Position = UDim2.fromOffset(tb.AbsolutePosition.X + tb.AbsoluteSize.X, tb.AbsolutePosition.Y + 20)
				ui.BackgroundTransparency = 0
				ui.BackgroundColor3 = Color3.fromRGB(59, 59, 59)
				ui.BorderSizePixel = 1
				ui.BorderColor3 = Color3.fromRGB(34, 34, 34)
				ui.Visible = true
				ui.Parent = SerializedTable.MainUI_Dashboard
				ui.ZIndex = 99999
				local absvals = {}
				task.wait()
				local thethr = UserInputService.InputBegan:Connect(function(k)
					if k.UserInputType==Enum.UserInputType.MouseButton1 then
						ui :Destroy()
					end 
				end)

				for nd,v in Value do
					local c:TextButton = ui.row:Clone()
					c.Text = nd
					c.Visible = true
					c.Parent = ui
					table.insert(absvals, c)
					
					c.MouseButton1Down:Connect(function()
						val = v
						valname = nd
						upd()
						ui:Destroy()
						thethr:Disconnect()
					end)
				end
				for _,v in absvals do
					v.Size = UDim2.new(0, ui.AbsoluteSize.X, 0, 20)
					v.AutomaticSize = Enum.AutomaticSize.None
				end
			end
		end)
		
	end
	if Style=="string" or Style=="number" then
		local def = Value.Value
		local Value:BoolValue = Value
		local Sheet:Frame = ListRow.textbox
		Sheet.Visible = true
		local i = Sheet:FindFirstChildWhichIsA("TextBox")
		
		local previoustext = ""
		i:GetPropertyChangedSignal("Text"):Connect(function()
			previoustext = i.Text
		end)
		i.FocusLost:Connect(function()
			if Style=="string" then
				Value.Value = i.Text
			else
				local t = tonumber(i.Text)

				if t then
					Value.Value = t
				else
					Value.Value = 0
					i.Text = "0"
				end
			end
			
			i.Text = Value.Value
		end)
		Value:GetPropertyChangedSignal("Value"):Connect(function()
			if not i:IsFocused() then
				i.Text = Value.Value
			end
		end)
		SerializedTable.MainUI_Dashboard.Destroying:Connect(function()
			Value.Value = def
		end)
	end
	
	
	return Label
end



--// Code

SetupInterface()


return module
